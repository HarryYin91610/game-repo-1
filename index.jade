html
  head
    meta(charset="utf-8")
    title Welcome to My Repo
    style.
      html {
        font-family: "微软雅黑";
      }

      ul {
        padding: 0;
        list-style: none;
      }

      li a {
        font-weight: bold;
        color: #4fc08d;
        text-decoration: none;
      }


  body
      ul
        each value, key in htmlWebpackPlugin.options.availableProjects
          li
            a(href="/#{value.filename}") #{key}

